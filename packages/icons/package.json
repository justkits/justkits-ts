{
  "name": "@justkits/icons",
  "description": "JustKits Icons Package",
  "private": false,
  "version": "0.0.2",
  "type": "module",
  "keywords": [
    "icons",
    "react",
    "typescript"
  ],
  "author": "JustWon",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justkits/justkits-ts.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "exports": {
    ".": {
      "types": "./dist/web/index.d.ts",
      "react-native": "./dist/native/index.js",
      "default": "./dist/web/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias --project tsconfig.build.json",
    "clean": "tsx ./scripts/clean.ts",
    "generate": "tsx ./scripts/generate.ts",
    "check": "tsx ./scripts/check.ts",
    "test": "vitest --coverage",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@justkits/svgs-core": "workspace:*",
    "@justkits/tsconfig": "workspace:*",
    "@justkits/vitest-config": "workspace:*",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "catalog:",
    "@types/react": "^19.2.7",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "fast-glob": "^3.3.3",
    "jsdom": "^27.3.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native-svg": ">=15.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-svg": {
      "optional": true
    }
  }
}
