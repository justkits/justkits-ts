{
  "name": "@justkits/icons",
  "description": "JustKits Icons Package",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "keywords": [
    "icons",
    "react",
    "typescript"
  ],
  "author": "JustWon",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justkits/justkits-ts.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "default": "./dist/*/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "generate": "tsx ./scripts/generate.ts && prettier --write src/",
    "test": "vitest run",
    "test:dev": "vitest --watch --coverage"
  },
  "devDependencies": {
    "@justkits/configs": "workspace:*",
    "@justkits/svgs-core": "workspace:*",
    "@svgr/core": "^8.1.0",
    "@testing-library/react": "^16.3.1",
    "@types/node": "catalog:",
    "@types/react": "^19.2.7",
    "@vitest/coverage-v8": "catalog:",
    "jsdom": "^27.3.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
