{
  "name": "@justkits/icons",
  "description": "JustKits Icons Package",
  "version": "0.0.1",
  "type": "module",
  "private": false,
  "keywords": [
    "icons",
    "react",
    "typescript"
  ],
  "author": "JustWon",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justkits/justkits-ts.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "exports": {
    ".": {
      "react-native": "./dist/native/index.js",
      "default": "./dist/web/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "tsx ./scripts/generate.ts",
    "build": "tsc && tsc-alias"
  },
  "devDependencies": {
    "@configs/typescript": "workspace:*",
    "@justkits/svgs-core": "workspace:*",
    "@types/react": "^19.2.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native-svg": ">=15.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-svg": {
      "optional": true
    }
  }
}
