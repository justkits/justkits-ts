{
  "name": "@justkits/svgs-core",
  "description": "JustKits SVGs Core Package",
  "version": "1.0.1",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "test": "vitest run",
    "test:dev": "vitest --watch --coverage"
  },
  "dependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "chalk": "^5.6.2",
    "fast-glob": "^3.3.3",
    "p-limit": "^6.1.0"
  },
  "devDependencies": {
    "@justkits/configs": "workspace:*",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "dir-compare": "^5.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
