{
  "name": "@justkits/svg2tsx",
  "description": "JustKits SVG to TSX Converter Package",
  "version": "1.0.3",
  "private": false,
  "type": "module",
  "keywords": [
    "icons",
    "react",
    "typescript"
  ],
  "bin": {
    "svg2tsx": "./dist/cli/index.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "docs",
    "README.md",
    "LICENSE"
  ],
  "author": "JustWon",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justkits/justkits-ts.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "test": "vitest run",
    "test:dev": "vitest --watch --coverage"
  },
  "devDependencies": {
    "@justkits/configs": "workspace:*",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "tsup": "^8.5.1",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "fast-glob": "^3.3.3",
    "jiti": "^2.6.1",
    "p-limit": "^7.2.0"
  }
}
